{
  "expo": {
    "name": "tour-360",
    "slug": "tour-360",
    "version": "0.0.6",
    "icon": "./assets/images/icon.png",
    "scheme": "tour360",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios"
    ],
    "backgroundColor": "#000000",
    "ios": {
      "supportsTablet": true,
      "isTabletOnly": true,
      "requireFullScreen": false
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7065009a-7bf6-41ce-afd5-4d94a8c367f1"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "nohr"
  }
}